<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="SmallHeadLine" parent="TextAppearance.Material3.HeadlineSmall" />

    <style name="MediumHeadline" parent="TextAppearance.Material3.HeadlineMedium" />

    <style name="LargeHeadline" parent="TextAppearance.Material3.HeadlineLarge" />

    <style name="SmallTitle" parent="TextAppearance.Material3.TitleSmall" />

    <style name="MediumTitle" parent="TextAppearance.Material3.TitleMedium" />

    <style name="LargeTitle" parent="TextAppearance.Material3.TitleLarge" />

    <style name="SmallBody" parent="TextAppearance.Material3.BodySmall" />

    <style name="MediumBody" parent="TextAppearance.Material3.BodyMedium" />

    <style name="LargeBody" parent="TextAppearance.Material3.BodyLarge" />

    <style name="App.Custom.Indicator" parent="Widget.Material3.BottomNavigationView.ActiveIndicator">
        <item name="android:color">@color/orange100</item>
    </style>

    <style name="ThemeOverlay.AppTheme.TextInputEditText.Outlined" parent="">
        <item name="colorPrimary">@color/orange200</item>
    </style>

    <style name="ShapeAppearanceOverlayExtended" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">100dp</item>
    </style>

    <style name="CustomNumberPickerStyle">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/orange200</item>
        <item name="android:textSize">30sp</item>
        <item name="colorControlNormal">@color/transparent</item>
    </style>

    <style name="CustomPopUpMenuStyle" parent="Widget.Material3.PopupMenu">
        <item name="android:popupBackground">@color/background_color</item>
    </style>

</resources>